{# _project_members.html.twig #}
<div class="card">
    <div class="card-header">
        Project Members
    </div>
    <ul class="list-group list-group-flush">
        {% for member in project.contributors %}
            <li class="list-group-item">{{ member.username }}</li>
        {% endfor %}
    </ul>
</div>
